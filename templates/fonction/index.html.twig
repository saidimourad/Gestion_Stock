{% extends 'base.html.twig' %}

{% block title %}Hello FonctionController!{% endblock %}

{% block body %}
    {% if fonctions %}
        <div class="row">
        <div class="col-lg-12">
        <div class="panel panel-default">
        <div class="panel-body">
        <div class="table-responsive">
        <div class="card-body">
            <h5 class="card-title m-b-0">Liste de fontions</h5>
        </div>
        <table id="zero_config" class="table table-striped table-bordered">
            <thead>
            <tr>
                <th>Fonction</th>

                <th>Action</th>

            </tr>
            </thead>
            <tbody>
            {% for fonction in fonctions %}

                <tr>
                    <td>{{ fonction.Fonction }}</td>



                    <td>

                        <a href="/fonction/edit/{{ fonction.id }}" data-toggle="tooltip" data-placement="top" title="Update">
                            <i class="mdi mdi-check"></i>
                        </a>
                        <a href="#" data-toggle="tooltip" data-placement="top" title="Delete" data-id="{{ fonction.id }}">
                            </i><i class="mdi mdi-close"></i>
                        </a>


                    </td>

                </tr>
            {% endfor %}
            </tbody>
        </table>

    {% else %}
        <p>No fonctions to display</p>
    {% endif %}
    </div>
    </div>
    <a href="/fonction/new"  class="btn btn-success">Ajouter fonction</a>
    </div>
    </div>
    </div>
    </div><!--/.row-->


    </div>
{% endblock %}

{% block javascripts %}




{% endblock %}